<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
	android:paddingRight="10dip"
	android:paddingLeft="10dip"
	android:background="@color/generic_white">
    <Button 
	    android:id="@+id/bt_wedge_del"
		android:layout_width="@dimen/delete_size"
		android:layout_height="@dimen/delete_size"
	    android:layout_alignParentRight="true"
	    android:layout_centerVertical="true"
	    android:layout_marginRight="3dip"
	    android:background="@drawable/item_del_selector"/>
    <TextView 
        android:id="@+id/tv_atmno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/generic_listview_item"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:background="@color/generic_transparent"/>

    <TextView 
	    android:id="@+id/tv_wedge_loc"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_centerVertical="true"
	    android:layout_toLeftOf="@id/bt_wedge_del"
	    android:layout_marginRight="5dip"
	    android:background="@color/generic_transparent"
	    style="@style/generic_listview_item"/>
    
    <TextView 
	    android:id="@+id/tv_wedge_mony"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_centerVertical="true"
	    android:layout_toLeftOf="@id/tv_wedge_loc"
	    android:layout_marginRight="10dip"
	    android:background="@color/generic_transparent"
	    style="@style/generic_listview_item"/>
    <TextView 
	    android:id="@+id/tv_wedge_code"
	    android:layout_width="wrap_content"
		android:layout_height="@dimen/add_atmcheck_repair"
	    android:layout_centerVertical="true"
	    android:layout_toLeftOf="@id/tv_wedge_mony"
	    android:layout_toRightOf="@id/tv_atmno"
	    android:layout_marginRight="3dip"
	    android:layout_marginLeft="10dip"
	    android:background="@color/generic_transparent"
	    style="@style/generic_listview_item"/>
    
</RelativeLayout>
